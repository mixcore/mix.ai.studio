# Mixcore API Configuration
VITE_MIXCORE_ENDPOINT=https://mixcore.net/api/v2
VITE_MIXCORE_PREVIEW_ENDPOINT=https://mixcore.net

# Optional: Custom token storage keys
VITE_MIXCORE_TOKEN_KEY=mix_access_token
VITE_MIXCORE_REFRESH_TOKEN_KEY=mix_refresh_token

# Demo mode (bypasses authentication for development)
VITE_DEMO_MODE=false

# Development mode
VITE_DEV_MODE=true

# Mixcore API base URL
VITE_MIXCORE_API_URL=https://mixcore.net
VITE_MIXCORE_PREVIEW_ENDPOINT=https://mixcore.net

# External LLM API Keys (add your actual keys here)
VITE_OPENAI_API_KEY=your-openai-api-key-here
VITE_CLAUDE_API_KEY=sk-ant-api03-...
VITE_GEMINI_API_KEY=your-google-gemini-api-key-here
VITE_DEEPSEEK_API_KEY=your-deepseek-api-key-here